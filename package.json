    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^22.4.3",
    "nodemon": "^1.17.4"
  },
  "dependencies": {
    "@koa/cors": "2",
    "@spksoft/koa-decorator": "^1.2.2",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "gelf-pro": "^1.2.2",
    "http-graceful-shutdown": "^2.1.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^3.0.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "mongoose": "^5.0.17",
    "stack-trace": "^0.0.10",
    "util": "^0.10.3",
    "winston": "^2.4.2",
